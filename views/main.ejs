    $('#refreshBtn').click(function(){
            $.ajax({
                    url:'http://localhost:3000/nowRate',
                    type : 'GET',
                success:function(data){
                    //var json = JSON.parse(data);
                    var country = Object.keys(data);
                    var nowrate = Object.values(data);
                    console.log(country);
                  var tbody = $('tbody');
                  $('td').remove();
                    /*
                  if($('td') != null)
                  {
                      console.log('notnull')
                      $('td').remove();
                  }
                  */
                for(var count=0; count<country.length; count++)
                {
                    var str = '<tr>' + '<td>' + country[count] + '</td>';
                    
                    var arr = nowrate[count];
                    for(var i=0; i<arr.length;i++) str = str + '<td>' + arr[i] + '</td>';
                    str = str + '</tr>';
                    tbody.append(str); 
                } 
                                
                }
            })
    });
        
</script>

